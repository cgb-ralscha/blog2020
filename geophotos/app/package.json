{
  "name": "geophotos",
  "version": "1.0.0",
  "app": "src/index.html",
  "targets": {
    "app": {
      "engines": {
        "browsers": "> 1%"
      },
      "distDir": "dist",
      "publicUrl": "./",
      "sourceMap": false
    }
  },
  "scripts": {
    "prestart": "rimraf dist/*.*",
    "start": "parcel --port 8080 src/index.html",
    "prebuild": "rimraf dist/*.*",
    "build": "parcel build src/index.html",
    "postbuild": "bread-compressor dist !*.jpg !*.jpeg",
    "serve-dist": "ws --hostname localhost -d dist -p 8080 -o --log.format stats"
  },
  "devDependencies": {
    "@babel/core": "7.15.5",
    "@babel/plugin-transform-runtime": "7.15.0",
    "@parcel/transformer-image": "^2.0.0-rc.0",
    "bread-compressor-cli": "1.1.0",
    "local-web-server": "5.1.1",
    "parcel": "2.0.0-rc.0",
    "rimraf": "3.0.2"
  },
  "dependencies": {
    "@googlemaps/markerclustererplus": "1.2.2",
    "date-fns": "2.24.0",
    "lg-autoplay.js": "1.2.0",
    "lg-fullscreen.js": "1.2.0",
    "lg-zoom.js": "1.3.0",
    "lightgallery.js": "1.4.0",
    "regenerator-runtime": "0.13.9"
  }
}
